{
  "name": "foundryforged",
  "version": "0.0.1",
  "description": "Accessible WordPress theme utilizing flex layouts and modern development practices.",
  "main": "index.php",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kbihler/foundryforged"
  },
  "author": "Kyle Bihler",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kbihler/foundryforged/issues"
  },
  "homepage": "https://github.com/kbihler/foundryforged#readme",
  "scripts": {
    "build:style": "node-sass sass/style.scss style.css --output-style expanded && postcss -r style.css",
    "build:rtl": "rtlcss style.css style-rtl.css",
    "build": "gulp sass",
    "watch": "chokidar \"**/*.scss\" -c \"npm run build\" --initial",
    "start": "gulp"
  },
  "dependencies": {
    "foundation-sites": "^6.5.1",
    "jquery": ">=3.0.0",
    "motion-ui": "~2.0.0",
    "what-input": "^5.1.2"
  },
  "devDependencies": {
    "@wordpress/browserslist-config": "^2.2.2",
    "autoprefixer": "^9.1.5",
    "chokidar-cli": "^1.2.1",
    "gulp": "^3.9.1",
    "gulp-image": "^2.7.2",
    "gulp-jshint": "^2.0.2",
    "gulp-newer": "^1.3.0",
    "gulp-postcss": "^6.2.0",
    "gulp-sass": "^2.3.2",
    "gulp-load-plugins": "^1.1.0",
    "gulp-sourcemaps": "^2.2.0",
    "jshint": "^2.9.4",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.0.1",
    "postcss-focus-within": "^3.0.0",
    "rtlcss": "^2.4.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
